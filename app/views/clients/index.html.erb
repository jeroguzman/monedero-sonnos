<table class="table">
  <tr>
    <th>Numero de cliente</th>
    <th>Nombre</th>
    <th>Telefono</th>
    <th>Modelo de telefono</th>
    <th>Fecha de nacimiento</th>
    <th>Correo Electronico</th>

  </tr>
  <% @clients.each do |client| %>
    <tr>
      <td><%= link_to client.client_number, client_path(client.id) %></td>
      <td><%= client.client_profile.name %></td>
      <td><%= client.client_profile.phone %></td>
      <td><%= client.client_profile.phone_model %></td>
      <td><%= client.client_profile.birthdate.strftime("%d-%m-%Y") %></td>
      <td><%= client.client_profile.email %></td>
    </tr>
  <% end %>
</table>
