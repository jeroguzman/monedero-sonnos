<p id="notice"><%= notice %></p>

<h1>Balance Informations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @balance_informations.each do |balance_information| %>
      <tr>
        <td><%= link_to 'Show', balance_information %></td>
        <td><%= link_to 'Edit', edit_balance_information_path(balance_information) %></td>
        <td><%= link_to 'Destroy', balance_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Balance Information', new_balance_information_path %>
